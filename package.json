{
  "name": "convertss-backend",
  "version": "1.0.0",
  "description": "Simple Socket.IO backend for AI chat demo and Serve scipt",
  "main": "index.js",
  "scripts": {
    "setup": "bash setup.sh",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "pm2:start": "pm2 start index.js --name convertss_backend --max-memory-restart 500M",
    "pm2:stop": "pm2 stop convertss_backend",
    "pm2:restart": "pm2 restart convertss_backend",
    "pm2:delete": "pm2 delete convertss_backend",
    "pm2:logs": "pm2 logs convertss_backend",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "dev:build": "cross-env NODE_ENV=development node build.js --watch",
    "build": "cross-env NODE_ENV=production node build.js",
    "build:dev": "cross-env NODE_ENV=development node build.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop convertss-backend",
    "prod:restart": "pm2 restart convertss-backend",
    "prod:logs": "pm2 logs convertss-backend",
    "prod:monitor": "pm2 monit",
    "prod:delete": "pm2 delete convertss-backend",
    "test": "node testAiChat.js",
    "test:embeddings": "node functions/getEmbeddings.js",
    "test:vector": "node functions/storeVecDb.js",
    "deploy": "npm run build && node deploy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.2",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.2",
    "nodemon": "^3.1.11",
    "openai": "^6.17.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.449",
    "socket.io": "^4.7.2",
    "terser": "^5.46.0",
    "tesseract.js": "^7.0.0",
    "url": "^0.11.4"
  }
}